import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
        int n = scan.nextInt();
        float f = scan.nextFloat();
        System.out.println(n);
        System.out.printf("%.2f",f);
    }
}
